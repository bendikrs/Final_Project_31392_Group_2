   "source": [
    "import os\n",
    "files = os.listdir('.') \n",
    "\n",
    "def make_anotation(filename):\n",
    "    txt_file = filename.split('.')[0] + '.txt'\n",
    "    with open(txt_file, 'w') as file:\n",
    "        text = f\"0 0.5 0.5 1 1\"\n",
    "        file.write(text)\n",
    "\n",
    "for filename in files:\n",
    "    make_anotation(filename)\n",
    "\n"
   ]
  }
